return e&&(e.startsWith("http://")||e.startsWith("https://"))}get jobId(){return(0,d.entityUrnToId)(Ember.get(this.args,"model.entityUrn"))}get relevanceReasons(){return Ember.get(this.args,"jobPostingRelevanceReasonsModel.details")||Ember.get(this.args,"jobPostingRelevanceReasonsModel.reasons.firstObject.details")}get companyId(){return(0,d.entityUrnToId)(Ember.get(this.args,"model.companyDetails.company.entityUrn"))}get universalName(){return Ember.get(this.args,"model.companyDetails.company.universalName")}get shouldShowAppliedJobsLink(){return Ember.get(this.args,"model.applyingInfo.applied")}get showApplyReminderNotice(){return!Ember.get(this.args,"model.applyingInfo.applied")&&(Ember.get(this.args,"candidatePending.elements.length")||Ember.get(this.args,"candidateReferred.elements.length"))}get showTopCardUpsell(){return this.args.jobDetailUpsellType===r.UPSELL_TYPE_TOPCARD&&Ember.get(this.args,"model.applies")>=this.minApplicantThreshold&&!this.args.isPremium}get showCompanyInsightsUpsell(){return Ember.get(this.args,"model.applies")<this.minApplicantThreshold&&this.args.companyInsightsUpsell&&!this.args.isPremium}get isClosed(){return Ember.get(this.args,"model.jobState")===l.JOB_STATES.CLOSED}get displaySearchConnectionsLink(){return(0,u.getFlavorType)(this.relevanceReasons)===b.IN_NETWORK}handleCloseButtonClick(){this.hoverableService.close(!0)
